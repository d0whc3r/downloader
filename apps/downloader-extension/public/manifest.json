{
  "manifest_version": 3,
  "name": "downloader-extension",
  "description": "",
  "version": "0.0.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "browser_style": true,
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    },
    "default_title": "downloader-extension",
    "default_popup": "index.html",
    "theme_icons": [
      {
        "light": "icons/16.png",
        "dark": "icons/16-dark.png",
        "size": 16
      },
      {
        "light": "icons/32.png",
        "dark": "icons/32-dark.png",
        "size": 32
      },
      {
        "light": "icons/48.png",
        "dark": "icons/48-dark.png",
        "size": 48
      },
      {
        "light": "icons/128.png",
        "dark": "icons/128-dark.png",
        "size": 128
      }
    ]
  }
}
